@lightblue: #f7f9fc;
@blue: #4b94f2;

body {
	background: @lightblue;
	color: @blue;
	margin-top: 43px;
}

body, h1, h2, h3, h4, h5, h6 {
	font-family: "Lato", Helvetica, Arial, sans-serif;
}

.right
{
	float: right;
}

.left {
	float: left;
}

.clearfix {
	clear: both;
	overflow: hidden;
}

nav {
	
	border-bottom: 2px solid @blue;
	padding-bottom: 20px;

	h1 {
		text-transform: uppercase;
		letter-spacing: 5px;
		font-weight: normal;
	}
	
	.vr
	{
		height: 30px;
		width: 0px;
		border-left: 1px solid @blue;
		margin: 0 25px -20px 25px;
	}

}

input
{
	margin: 0 5px 0 5px;
}

.search {
	border: 1px solid @blue;
	border-radius: 100px;
	
	input {
		background: transparent;
		border: 0px solid black;
		line-height: 28px;
	}
}

.commission-panel {
  border-width: 1.5pt;
  border-radius: 4pt;
  position: relative;
  border-color: @blue;
}
.commissions-list ul {
  padding-left: 0;
}
.commission {
  padding-left: 1.5em;
  position: relative;
}
.selected-commission {
  background: @lightblue;
}
.commission-with-unread-messages::before {
  content: '';
  position: absolute;
  top: 0;
  left: 0;
  height: 100%;
  width: 1em;
  background: black;
}
